<Machine>
	<State name="STAND"/>
	<State name="RIGHT_SWING"/>
	<State name="LEFT_SWING"/>
	<State name="RIGHT_CATCH"/>
	<State name="LEFT_CATCH"/>
	<Event name="start_right" from="STAND" to="RIGHT_SWING"/>
	<Event name="start_left" from="STAND" to="LEFT_SWING"/>
	<Event name="right_throw" from="RIGHT_SWING" to="LEFT_CATCH"/>
	<Event name="left_throw" from="LEFT_SWING" to="RIGHT_CATCH"/>
	<Event name="right_swing_stop" from="RIGHT_SWING" to="STAND"/>
	<Event name="left_swing_stop" from="LEFT_SWING" to="STAND"/>
	<Event name="right_catch_stop" from="RIGHT_CATCH" to="STAND"/>
	<Event name="left_catch_stop" from="LEFT_CATCH" to="STAND"/>
</Machine>